fun fib(n) {
  if (n <= 1) return n;
  return fib(n - 2) + fib(n - 1);
}

var start = clock();

for (var i = 0; i < 20; i = i + 1) {
  print fib(i);
}

var end = clock();

print "";
print "Time to run fibonacci:";
print end - start;
print "";

// closures example
fun count(n) {
  while (n < 100) {
    if (n == 3) return n;
    print n;
    n = n + 1;
  }
}

count(1);

